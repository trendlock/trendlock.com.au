{
  "name": "trendlock.com.au",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "gulp",
    "build": "bankai build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "xo && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons ",
    "deploy": "npm run build && now --name trendlock dist/"
  },
  "dependencies": {
    "choo": "^6.8.0",
    "choo-devtools": "^2.3.4",
    "choo-service-worker": "^2.4.0",
    "feather-icons": "^4.6.0",
    "nanocomponent": "^6.5.1",
    "on-intersect": "^2.0.0",
    "raf": "^3.4.0",
    "sheetify": "^7.2.0",
    "tachyons": "^4.9.1",
    "universal-ga": "^1.2.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "bankai": "^9.7.2",
    "choo-scaffold": "^1.1.2",
    "del": "^3.0.0",
    "dependency-check": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-responsive": "^2.9.0",
    "hstream": "^1.1.0",
    "typeface-pt-mono": "0.0.54",
    "xo": "^0.20.3"
  },
  "documentify": {
    "transform": [
      "./transforms/favicon"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "space": true,
    "prettier": true,
    "rules": {
      "no-unused-expressions": [
        "error",
        {
          "allowTaggedTemplates": true
        }
      ]
    }
  }
}
